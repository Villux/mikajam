.well.masTeam
  margin-top: 55px
  border-color: rgba(29, 141, 204, 1)
  border-style: solid
  border-width: 5px
  border-radius: 20px
  position: relative
  background: -webkit-radial-gradient(center, ellipse cover,  rgba(125,126,125,1) 0%,rgba(14,14,14,0.8) 58%,rgba(14,14,14,1) 100%); /* Chrome10+,Safari5.1+ */



.well.conTeam
  border-color: rgba(255, 66, 61, 1)
  border-style: solid
  border-width: 5px
  border-radius: 20px
  background: -webkit-radial-gradient(center, ellipse cover,  rgba(125,126,125,1) 0%,rgba(14,14,14,0.8) 58%,rgba(14,14,14,1) 100%); /* Chrome10+,Safari5.1+ */

