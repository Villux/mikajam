html
  overflow: auto !important
body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  background-image: url("/images/bballtexture.png")

#mobile
  margin-top: 40px
  margin-bottom: 40px
  margin-right: 20px
  margin-left: 20px
  background: rgba(33, 29, 41, 0.95)
  display:block
  color: white
  border-radius: 20px
  text-align: center
  font-size: medium
#mobile p
  font-family: quicksand
.air
  font-family: airstream
  font-size: 3em


.vsContainer
  position: relative
  overflow: hidden;
  width: 100%;
.vsText
  height: 100% !important
  position: absolute
  bottom: 0
//menu stuff
a
  color: #00B7FF
a
	text-decoration:none;
#menu li
  display: inline-block
  margin: 10px
  background: rgba(0,0,0, 0.8)
#menu li
  -webkit-border-radius: 10px
  border-radius: 10px
  min-width: 150px
  text-align: center
  //new background
  border-color: white
  border-style:solid
  border-width: 1px
#menu li.active
  // background: #fff
  background: rgba(255,255,255, 0.8)
  color: #fff
#menu li a
  text-decoration: none
  color: #fff
#menu li .aMas:hover
  background: rgba(29, 141, 204, 0.9)
  border-radius: 10px
#menu li .aCon:hover
  background: rgba(255, 66, 61, 0.9)
  border-radius: 10px
#menu li.active .aMas:hover
  background: rgba(29, 141, 204, 0.9)
  border-radius: 10px
#menu li.active .aCon:hover
  background: rgba(255, 66, 61, 0.9)
  border-radius: 10px
#menu li a,
#menu li.active a
  padding: 9px 18px
  display:block
#menu li.active a
  color: #000
#menu
  position: fixed
  top: 0
  left: 0
  height: 40px
  z-index: 70
  width: 100%
  padding-left: 5px
  padding-right: 5px
  margin: 0

// fullPage stuff
#section1, #section2, #section3, #section4
  background-size: cover
  background-image: url("/images/bballtexture.png")

#page1h1
  z-index 1;
  position: absolute;
  left: 45%
  bottom: 50%
  text-align: center;

.fltext
  z-index 1
  position: absolute
  top: 80px
  background: rgba(255,255,255, 0.5)
  -moz-border-radius-topright: 10px
  -webkit-border-top-right-radius: 10px
  border-top-right-radius: 10px
  border-bottom-right-radius: 10px
  -moz-border-radius-bottomright: 10px
  -webkit-border-bottom-right-radius: 10px

#section1 #pheader
  text-align: center
#pheader
  width: 98%
  // bottom: 0
  top: 0
  // margin-top: 75px
  position: fixed
  // background: rgba(29, 141, 204, 0.79)
  background: rgba(0, 0, 0, 0.79)
  padding-left: 5%
  padding-right: 5%
  padding-bottom: 10px
  font-size: 5.5em
  display: none
  // margin-top: 75px
  // margin-top: 45px
  border-radius: 15px
  margin-left: 13px
  font-family: airstream;
  color: rgba(211, 211, 211, 0.79)

