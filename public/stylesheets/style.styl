html
  overflow: auto !important
body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  background-image: url("/images/asphalt_texture_mobile.jpg")

#mobile
  background: rgba(33, 29, 41, 0.90)
  background-size: cover
  width: 100% !important
  display:block
  color: white
  text-align: center
  font-size: 3em
#mobile p
  font-family: quicksand
.air
  font-family: airstream
  font-size: 3em
.airH
  font-family: airstream
  font-size: 3.5em


.vsContainer
  position: relative
  overflow: hidden;
  width: 100%;
.vsText
  height: 100% !important
  position: absolute
  bottom: 0

.info
  font-family: quicksand
  font-size: 0.6em 

@font-face {
    font-family: airstream;
    src: url("/stylesheets/Airstream-webfont.woff");
}
@font-face {
    font-family: quicksand;
    src: url("/stylesheets/Quicksand-Light.otf");
}
@media screen and (min-width: 998px)
  @font-face {
    font-family: quicksandRegular;
    src: url("/stylesheets/Quicksand-Regular.otf");
  }
