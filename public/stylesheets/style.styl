.masTeam div .thumbnail:hover
  border-color: #ffffff

.conTeam div .thumbnail:hover
  border-color: #ffffff

.masContainer
  position: relative
  background: rgba(195, 200, 202, 0.94)
  border-radius: 20px
  border-bottom-right-radius: 0px
  border-bottom-left-radius: 0px

.conContainer
  position: relative
  background: rgba(195, 200, 202, 0.94)
  border-radius: 20px
  border-top-right-radius: 0px
  border-top-left-radius: 0px

.masTeamText
  position: absolute
  font-family: airstream
  // padding-top: 10%
  margin-top: 55px

.conTeamText
  position: absolute
  font-family: airstream


.thumbnailTextMas
  // padding-top: 20%
  opacity: 0
  color: white
  top: 0
  left: 0
  position: absolute
  width: 100%
  height: 100%

.thumbnailTextCon
  opacity: 0
  color: white
  top: 0
  left: 0
  position: absolute
  width: 100%
  height: 100%

.thumbnailTextMas:hover
  background-color: rgba(29, 141, 204, 1)
  font-family: airstream;
  font-size: 1.3em
  opacity: 1
  border-radius: 3px
.thumbnailTextCon:hover
  background-color: rgba(255, 66, 61, 1)
  font-family: airstream;
  font-size: 1.5em
  opacity: 1
  border-radius: 3px

div .thumbnail
  background: none
  border-width: 0px
.photoContainer
  position: relative
  text-align: center
  background: rgba(255, 255, 255, 0.2) !important
